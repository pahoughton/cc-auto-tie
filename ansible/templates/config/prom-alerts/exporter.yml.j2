---
groups:
  - name: exporters
    rules:
      - alert: exporter-down
        expr: up == 0
        labels:
          {% raw -%}
          ansible: exporter-{{ $labels.service }}
          {% endraw -%}
        annotations:
          {% raw %}
          description: "{{ $labels.service }} is down"
          {% endraw %}
